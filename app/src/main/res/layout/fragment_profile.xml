<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment">

    <!-- TODO: Update blank fragment layout -->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f5f5f5">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="844dp">
            <ImageView
                android:id="@+id/Vector"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/vector"
                android:layout_marginStart="31dp"
                android:layout_marginTop="64dp"/>
            <TextView
                android:id="@+id/my_account"
                android:layout_width="93dp"
                android:layout_height="31dp"
                android:layout_marginLeft="62dp"
                android:layout_marginTop="62dp"
                android:text="@string/my_account"
                android:textSize="16sp"
                android:textStyle="bold"
                />

            <ImageButton
                android:id="@+id/ellipsebtn"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:src="@drawable/add"
                android:layout_marginLeft="31dp"
                android:layout_marginTop="55dp"
                android:layout_below="@+id/my_account"
                android:contentDescription="@string/xyz"
                />

            <ImageView
                android:id="@+id/ellipse"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="31dp"
                android:layout_marginTop="55dp"
                android:layout_below="@+id/my_account"

                />


            <TextView
                android:id="@+id/Olivia"
                android:layout_width="86dp"
                android:layout_height="31dp"
                android:layout_marginTop="156dp"
                android:layout_marginLeft="130dp"
                android:text="Olivia Grace"
                android:textStyle="bold"
                android:textSize="16dp"
                android:paddingTop="5dp"/>
            <ImageView
                android:id="@+id/chevron"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/chevron_right"
                android:layout_toRightOf="@+id/Olivia"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="162dp"/>
            <TextView
                android:id="@+id/mail"
                android:layout_width="137dp"
                android:layout_height="20dp"
                android:layout_marginTop="1dp"
                android:layout_marginLeft="130dp"
                android:layout_below="@+id/Olivia"
                android:text="Olivia.grace45@gmail.com"
                android:textSize="11sp"/>

            <View
                android:id="@+id/rectangle"
                android:layout_width="411dp"
                android:layout_height="7dp"
                android:layout_below="@+id/ellipse"
                android:layout_marginTop="14dp"
                android:background="#d9d9d9"
                />

            <RelativeLayout
                android:id="@+id/nested"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rectangle"
                android:layout_centerHorizontal="true"

                >

                <ImageView
                    android:id="@+id/clipboard"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/clipboard"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="42dp"
                    android:contentDescription="" />

                <TextView
                    android:id="@+id/previous_booking"
                    android:layout_width="122dp"
                    android:layout_height="27dp"
                    android:layout_marginTop="39dp"
                    android:layout_marginLeft="22dp"
                    android:text="@string/previous_bookings"
                    android:textSize="14sp"
                    android:layout_toRightOf="@+id/clipboard"
                    android:paddingTop="5dp"
                    />
                <ImageView
                    android:id="@+id/chevron1"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_marginLeft="127dp"
                    android:layout_marginTop="39dp"
                    android:layout_toRightOf="@+id/previous_booking"
                    />
                <ImageView
                    android:id="@+id/line1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@+id/previous_booking"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="14sp"/>

                <ImageView
                    android:id="@+id/star"
                    android:layout_width="20dp"
                    android:layout_height="19.02dp"
                    android:src="@drawable/star"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="99dp"/>
                <TextView
                    android:id="@+id/my_ratings"
                    android:layout_width="74dp"
                    android:layout_height="27dp"
                    android:layout_marginTop="94dp"
                    android:layout_marginLeft="22dp"
                    android:layout_toRightOf="@+id/star"
                    android:text="@string/my_ratings"
                    android:paddingTop="5dp"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/chevron2"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_toRightOf="@+id/my_ratings"
                    android:layout_marginLeft="180dp"
                    android:layout_marginTop="94dp"/>
                <ImageView
                    android:id="@+id/line2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@+id/my_ratings"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="14sp"/>




                <ImageView
                    android:id="@+id/send"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/send"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="207dp"/>
                <TextView
                    android:id="@+id/address"
                    android:layout_width="71dp"
                    android:layout_height="27dp"
                    android:layout_toRightOf="@+id/send"
                    android:layout_marginTop="204dp"
                    android:layout_marginLeft="22dp"
                    android:text="@string/addresses"
                    android:paddingTop="5dp"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/chevron4"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_toRightOf="@+id/address"
                    android:layout_marginLeft="179dp"
                    android:layout_marginTop="204dp"/>
                <ImageView
                    android:id="@+id/line4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@id/address"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="14sp"/>

                <ImageView
                    android:id="@+id/gift"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="262dp"
                    android:src="@drawable/gift_icon" />

                <TextView
                    android:id="@+id/members"
                    android:layout_width="90dp"
                    android:layout_height="27dp"
                    android:layout_toRightOf="@+id/gift"
                    android:layout_marginTop="259dp"
                    android:layout_marginLeft="22dp"
                    android:text="@string/memberships"
                    android:paddingTop="5dp"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/chevron5"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_toRightOf="@+id/members"
                    android:layout_marginLeft="158dp"
                    android:layout_marginTop="259dp"/>
                <ImageView
                    android:id="@+id/line5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@id/members"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="14sp"/>

                <ImageView
                    android:id="@+id/users"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/users"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="316dp"/>
                <TextView
                    android:id="@+id/work"
                    android:layout_width="78dp"
                    android:layout_height="27dp"
                    android:layout_toRightOf="@+id/users"
                    android:layout_marginTop="314dp"
                    android:layout_marginLeft="22dp"
                    android:text="@string/work_for_us"
                    android:paddingTop="5dp"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/chevron6"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_toRightOf="@+id/work"
                    android:layout_marginLeft="170dp"
                    android:layout_marginTop="314dp"/>
                <ImageView
                    android:id="@+id/line6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@id/work"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="14sp"/>

                <ImageView
                    android:id="@+id/help"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/help_circle"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="370dp"/>
                <TextView
                    android:id="@+id/help_input"
                    android:layout_width="31dp"
                    android:layout_height="27dp"
                    android:layout_toEndOf="@+id/help"
                    android:layout_marginTop="369dp"
                    android:layout_marginStart="22dp"
                    android:text="@string/help"
                    android:paddingTop="5dp"
                    android:textSize="14sp"/>
                <ImageView
                    android:id="@+id/chevron7"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/chevron_right"
                    android:layout_toEndOf="@+id/help_input"
                    android:layout_marginStart="217dp"
                    android:layout_marginTop="369dp"/>
                <ImageView
                    android:id="@+id/line7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/line4"
                    android:layout_below="@id/help_input"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="14sp"/>

            </RelativeLayout>

            <Button
                android:id="@+id/btn_sign_out"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_below="@+id/nested"
                android:layout_marginStart="39dp"
                android:layout_marginTop="33dp"
                android:backgroundTint="@color/black"
                android:text="@string/sign_out"
                android:textColor="#FFFFFF"
                android:layout_centerHorizontal="true"/>


        </RelativeLayout>

    </ScrollView>

</FrameLayout>